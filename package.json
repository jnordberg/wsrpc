    "@types/node": "^12.11.5",
    "@types/verror": "^1.9.3",
    "@types/ws": "^6.0.3",
    "coveralls": "^3.0.7",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.4"
  }
