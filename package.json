{
  "name": "wsrpc",
  "version": "1.4.1",
  "description": "node.js/browser protobuf rpc over binary websockets",
  "author": "Johan Nordberg",
  "license": "BSD-3-Clause",
  "main": "./lib6/index",
  "typings": "./lib6/index",
  "browser": {
